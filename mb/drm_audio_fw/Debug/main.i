void load_song_md() {
    s.song_md.md_size = c->song.md.md_size;
    s.song_md.owner_id = c->song.md.owner_id;
    s.song_md.num_regions = c->song.md.num_regions;
    s.song_md.num_users = c->song.md.num_users;
    memcpy(s.song_md.rids, (void *)(c->song.md.buf), s.song_md.num_regions);
    memcpy(s.song_md.uids, (void *)(c->song.md.buf + c->song.md.num_regions), s.song_md.num_users);
}



int is_locked() {
    int locked = 1U;


    if (!s.logged_in) {
        xil_printf("\r\nMB> " "No user logged in");
    } else {
        load_song_md();


        if (s.uid == s.song_md.owner_id) {
            locked = 0U;
        } else {
            for (int i = 0; i < 3 && locked; i++) {
                if (s.uid == s.song_md.uids[i]) {
                    locked = 0U;
                }
            }
        }

        if (locked) {
            xil_printf("\r\nMB> " "User '%s' does not have access to this song", s.username);
            return locked;
        }
        xil_printf("\r\nMB> " "User '%s' has access to this song", s.username);
        locked = 1U;


        for (int i = 0; i < s.song_md.num_regions; i++) {
            for (int j = 0; j < (u8)2; j++) {
                if (PROVISIONED_RIDS[j] == s.song_md.rids[i]) {
                    locked = 0U;
                }
            }
        }

        if (!locked) {
            xil_printf("\r\nMB> " "Region Match. Full Song can be played. Unlocking...");
        } else {
            xil_printf("\r\nMB> " "Invalid region");
        }
    }
    return locked;
}





int gen_song_md(char *buf) {
    buf[0] = ((5 + s.song_md.num_regions + s.song_md.num_users) / 2) * 2;
    buf[1] = s.song_md.owner_id;
    buf[2] = s.song_md.num_regions;
    buf[3] = s.song_md.num_users;
    memcpy(buf + 4, s.song_md.rids, s.song_md.num_regions);
    memcpy(buf + 4 + s.song_md.num_regions, s.song_md.uids, s.song_md.num_users);

    return buf[0];
}







void login() {
    if (s.logged_in) {
        xil_printf("\r\nMB> " "Already logged in. Please log out first.\r\n");
        memcpy((void*)c->username, s.username, 64);
        memcpy((void*)c->pin, s.pin, 64);
    } else {
        for (int i = 0; i < 3; i++) {

            if (!strcmp((void*)c->username, USERNAMES[PROVISIONED_UIDS[i]])) {

                if (!strcmp((void*)c->pin, PROVISIONED_PINS[i])) {

                    s.logged_in = 1;
                    c->login_status = 1;
                    memcpy(s.username, (void*)c->username, 64);
                    memcpy(s.pin, (void*)c->pin, 64);
                    s.uid = PROVISIONED_UIDS[i];
                    xil_printf("\r\nMB> " "Logged in for user '%s'\r\n", c->username);
                    return;
                } else {

                    xil_printf("\r\nMB> " "Incorrect pin for user '%s'\r\n", c->username);
                    memset((void*)c->username, 0, 64);
                    memset((void*)c->pin, 0, 64);
                    return;
                }
            }
        }


        xil_printf("\r\nMB> " "User not found\r\n");
        memset((void*)c->username, 0, 64);
        memset((void*)c->pin, 0, 64);
    }
}



void logout() {
