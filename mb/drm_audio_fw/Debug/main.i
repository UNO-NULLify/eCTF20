    }


    xil_printf("\r\nMB> " "\r\nMB> " "Dumping song (%dB)...", c->song.wav_size);
    memmove((void *)&c->song.md, (void *)((char *)(&c->song.md) + c->song.md.md_size), c->song.wav_size);

    xil_printf("\r\nMB> " "Song dump finished\r\n");
}





int main() {
    u32 status;
    *cmdreg |= 0xFFFFFFFF;

    init_platform();
    microblaze_register_handler((XInterruptHandler)myISR, (void *)0);
    microblaze_enable_interrupts();


    status = XIntc_Initialize(&InterruptController, 0);
    if (status != 0L) {
        return 1L;
    }


    status = SetUpInterruptSystem(&InterruptController, (XInterruptHandler)myISR);
    if (status != 0L) {
        return 1L;
    }


    status = fnConfigDma(&sAxiDma);
    if(status != 0L) {
        xil_printf("\r\nMB> " "DMA configuration ERROR\r\n");
        return 1L;
    }


    enableLED(led);
    c->drm_state = STOPPED; setLED(led, RED);;


    memset((void*)c, 0, sizeof(cmd_channel));

    xil_printf("\r\nMB> " "Audio DRM Module has Booted (Modified)\n\r");



    while(1) {

        if (InterruptProcessed) {
         uint32_t cmd = read_cr();
            InterruptProcessed = 0U;
            xil_printf("\r\nMB> " "Command: %08X\r\n", cmd);

            c->drm_state = WORKING; setLED(led, YELLOW);;

            switch (cmd) {
            case LOGIN:
                login();
                break;
            case LOGOUT:
                logout();
                break;
            case QUERY_PLAYER:
                query_player();
                break;
            case QUERY_SONG:
                query_song();
                break;
            case SHARE:
                share_song();
                break;
            case PLAY:
                play_song();
                xil_printf("\r\nMB> " "Done Playing Song\r\n");
                break;
            case DIGITAL_OUT:
                digital_out();
                break;
            default:
                break;
            }


            strcpy((char *)c->username, s.username);
            c->login_status = s.logged_in;
            usleep(500);
            c->drm_state = STOPPED; setLED(led, RED);;
        }
    }

    cleanup_platform();
    return 0;
}
