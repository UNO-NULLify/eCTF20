void share_song() {
    int new_md_len, shift;
    char new_md[256], uid;


    load_song_md();
    if (!s.logged_in) {
        xil_printf("\r\nMB> " "No user is logged in. Cannot share song\r\n");
        c->song.wav_size = 0;
        return;
    } else if (s.uid != s.song_md.owner_id) {
        xil_printf("\r\nMB> " "User '%s' is not song's owner. Cannot share song\r\n", s.username);
        c->song.wav_size = 0;
        return;
    } else if (!username_to_uid((char *)c->username, &uid, 1U)) {
        xil_printf("\r\nMB> " "Username not found\r\n");
        c->song.wav_size = 0;
        return;
    }


    s.song_md.uids[s.song_md.num_users++] = uid;
    new_md_len = gen_song_md(new_md);
    shift = new_md_len - s.song_md.md_size;


    if (shift) {
        memmove((void *)((char *)(&c->song.md) + c->song.md.md_size) + shift, (void *)((char *)(&c->song.md) + c->song.md.md_size), c->song.wav_size);
    }
    memcpy((void *)&c->song.md, new_md, new_md_len);


    c->song.file_size += shift;
    c->song.wav_size += shift;

    xil_printf("\r\nMB> " "Shared song with '%s'\r\n", c->username);
}



